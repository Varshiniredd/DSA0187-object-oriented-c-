#include <iostream>

int main() {
   
    int integerValue = 10;

    float floatValue;

    // Implicit conversion: Assign the integer variable to the float variable
    floatValue = integerValue;

    // Display the values
    std::cout << "Integer value: " << integerValue << std::endl;
    std::cout << "Float value (implicitly converted from integer): " << floatValue << std::endl;

    return 0;
}
