#include <iostream>

int main() {
    // Declare and initialize two integer variables
    int num1 = 10;
    int num2 = 20;

    // Perform addition using the + operator
    int sum = num1 + num2;

    // Display the result
    std::cout << "The sum of " << num1 << " and " << num2 << " is: " << sum << std::endl;

    return 0;
}
