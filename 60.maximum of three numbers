#include <iostream>

int main() {
    // Declare three variables to store the three numbers
    double num1, num2, num3;

    // Prompt the user to enter the three numbers
    std::cout << "Enter three numbers: ";
    std::cin >> num1 >> num2 >> num3;

    // Find the maximum using conditional statements
    double max = num1;

    if (num2 > max) {
        max = num2;
    }

    if (num3 > max) {
        max = num3;
    }

    // Display the maximum number
    std::cout << "The maximum of the three numbers is: " << max << std::endl;

    return 0;
}
