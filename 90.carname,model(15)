#include <iostream>
#include <string>

class Car {
private:
    std::string model;

public:
    
    Car() : model("Unknown") {}

    
    std::string getModel() const {
        return model;
    }
};

int main() {
    
    Car myCar;

  
    std::cout << "Model of the car: " << myCar.getModel() << std::endl;

    return 0;
}
